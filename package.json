{
  "name": "higlass-app",
  "description": "A fully-featured app for viewing, exploring, and annotating 1D and 2D genomic data",
  "version": "0.7.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hms-dbmi/higlass-app.git"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "deep-equal": "1.0.1",
    "higlass": "0.9.8",
    "history": "4.6.3",
    "localforage": "1.5.3",
    "pixi.js": "4.6.0",
    "prop-types": "15.6.0",
    "react": "15.6.1",
    "react-bootstrap": "0.31.5",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "3.7.2",
    "redux-batched-actions": "0.2.0",
    "redux-freeze": "0.1.5",
    "redux-logger": "3.0.6",
    "redux-persist": "4.10.2",
    "redux-thunk": "2.2.0",
    "redux-undo": "1.0.0-beta9-9-6"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "0.3.3",
    "autoprefixer": "7.1.6",
    "autotrack": "2.4.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-app": "3.1.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "change-case": "3.0.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-react-app": "2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "markdown-jsx-loader": "jquense/markdown-jsx-loader#v3.0.1",
    "marked": "0.3.6",
    "node-sass": "4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "react-dev-utils": "4.2.1",
    "react-error-overlay": "3.0.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "npm run build-wiki && node scripts/start.js",
    "build": "npm run build-wiki && node scripts/build.js",
    "build-travis": "npm run build-wiki && node scripts/build.js && zip -r build.zip build",
    "test": "npm run build-wiki",
    "build-wiki": "npm run update-wiki && node scripts/wiki.js",
    "update-wiki": "git submodule update --init --recursive --remote --merge"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
