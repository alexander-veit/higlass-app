{
  "name": "higlass-app",
  "description": "A fully-featured app for viewing, exploring, and annotating 1D and 2D genomic data",
  "version": "0.9.3",
  "author": "Fritz Lekschas (https://lekschas.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hms-dbmi/higlass-app.git"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "deep-equal": "1.0.1",
    "higlass": "1.2.3",
    "history": "4.7.2",
    "pixi.js": "4.8.2",
    "prop-types": "15.6.2",
    "pub-sub-es": "1.0.0",
    "react": "16.5.2",
    "react-bootstrap": "0.32.1",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-twitter-widgets": "^1.7.1",
    "redux": "3.7.2",
    "redux-batched-actions": "0.4.1",
    "redux-freeze": "0.1.5",
    "redux-logger": "3.0.6",
    "redux-persist": "4.10.2",
    "redux-thunk": "2.3.0",
    "redux-undo": "1.0.0-beta9-9-7"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autodll-webpack-plugin": "0.4.2",
    "autoprefixer": "9.1.5",
    "autotrack": "2.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-preset-react-app": "5.0.2",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "1.1.3",
    "change-case": "3.0.2",
    "css-loader": "0.28.11",
    "deasync": "0.1.13",
    "dotenv": "6.0.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-app": "3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "23.6.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "node-sass": "^4.9.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "react-dev-utils": "5.0.1",
    "react-error-overlay": "5.0.3",
    "sass-loader": "7.1.0",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "0.23.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build-travis": "node scripts/build.js && cd build && zip -r ../build.zip ./*",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "fix": "eslint --fix src"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  }
}
